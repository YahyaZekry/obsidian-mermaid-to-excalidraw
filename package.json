{
  "name": "obsidian-mermaid-to-excalidraw",
  "version": "1.0.0",
  "description": "Convert Mermaid diagrams to Excalidraw drawings in Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest"
  },
  "keywords": [
    "obsidian",
    "mermaid",
    "excalidraw",
    "diagram",
    "converter"
  ],
  "author": "Yahya Zekry",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_USERNAME/YOUR_REPOSITORY_NAME.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/YOUR_REPOSITORY_NAME/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/YOUR_REPOSITORY_NAME#readme",
  "dependencies": {
    "@excalidraw/excalidraw": "0.17.1-7381-cdf6d3e",
    "@excalidraw/markdown-to-text": "0.1.2",
    "mermaid": "10.9.0",
    "nanoid": "4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^16.11.6",
    "@types/mermaid": "9.2.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
